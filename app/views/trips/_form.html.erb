<%= form_for @trip do |f|%>
<%= f.label :name %>:
<%= f.text_field :name %><br />
<%= f.label :location %>
<%= f.text_field :location %><br />
<%= f.label :description%>
<%= f.text_area :description%>
<%= f.label :start_date %>
<%= f.date_field :start_date %>
<%= f.label :end_date %>
<%= f.date_field :end_date %>
<%= f.submit%>
<% end %>
