<section class="form">
  <h1>Login</h1>
  <article class="form-wrapper">
    <%=form_for(:session, url: login_path) do |f|%>
      <%= render partial: '/errors' %>
      <div class="form-section">
        <%= f.label :email %>
        <%= f.email_field :email %>
      </div>
      <div class="form-section">
        <%= f.label :password %>
        <%= f.password_field :password %>
      </div>
      <div class="form-section">
        <%= f.submit "Login"%>
      </div>
    <% end %>
  </article>
</section>
